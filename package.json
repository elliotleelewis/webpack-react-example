{
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --mode production",
    "prettier": "prettier --check \"**/*.{md,ts,js,tsx,jsx,scss,html,json,yml}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.{md,ts,js,tsx,jsx,scss,html,json,yml}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "html-webpack-plugin": "^5.0.0",
    "prettier": "~2.5.0",
    "ts-loader": "^9.0.0",
    "tslib": "~2.3.0",
    "typescript": "~4.5.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.0.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.json",
          "*.md",
          "*.yml"
        ],
        "options": {
          "tabWidth": 2,
          "useTabs": false
        }
      }
    ],
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  }
}
